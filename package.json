{
  "name": "flexible-gatsby",
  "private": true,
  "description": "A starter for a paginated blog powered by Gatsby and Markdown",
  "version": "1.1.0",
  "author": "Kinyanjui Wangonya",
  "bugs": {
    "url": "https://github.com/wangonya/flexible-gatsby/issues"
  },
  "dependencies": {
    "gatsby": "^2.24.63",
    "gatsby-image": "^2.4.19",
    "gatsby-plugin-feed": "^2.5.12",
    "gatsby-plugin-google-analytics": "^2.3.14",
    "gatsby-plugin-manifest": "^2.4.30",
    "gatsby-plugin-offline": "^3.2.28",
    "gatsby-plugin-react-helmet": "^3.3.11",
    "gatsby-plugin-sass": "^2.3.13",
    "gatsby-plugin-sharp": "^2.6.36",
    "gatsby-remark-copy-linked-files": "^2.3.15",
    "gatsby-remark-images": "^3.3.30",
    "gatsby-remark-katex": "^3.3.12",
    "gatsby-remark-prismjs": "^3.5.13",
    "gatsby-source-filesystem": "^2.3.30",
    "gatsby-transformer-remark": "^2.8.35",
    "gatsby-transformer-sharp": "^2.5.15",
    "katex": "^0.12.0",
    "lodash": "^4.17.20",
    "node-sass": "^4.14.1",
    "prismjs": "^1.21.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "eslint": "^7.9.0",
    "eslint-plugin-react": "^7.20.6",
    "prettier": "^2.1.2"
  },
  "homepage": "https://github.com/wangonya/flexible-gatsby",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wangonya/flexible-gatsby.git"
  },
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "develop": "gatsby develop",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\"",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md'",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix gatsby-node.js",
    "deploy": "gh-pages -d public"
  }
}
